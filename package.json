{
  "name": "express-todo",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "migrate": "$(npm bin)/sequelize db:migrate",
    "seed": "$(npm bin)/sequelize --env=test db:migrate && $(npm bin)/sequelize --env=test db:seed:all",
    "start": "node ./bin/www",
    "test": "$(npm bin)/mocha ./test/todo.js"
  },
  "dependencies": {
    "body-parser": "~1.15.0",
    "cookie-parser": "~1.4.0",
    "debug": "~2.2.0",
    "express": "~4.13.1",
    "morgan": "~1.7.0",
    "pg": "^4.4.1",
    "pg-hstore": "^2.3.2",
    "sequelize": "^3.8.0",
    "serve-favicon": "~2.3.0"
  },
  "devDependencies": {
    "chai": "^3.4.1",
    "mocha": "^2.3.4",
    "sequelize-cli": "~2.3.1",
    "sqlite3": "^3.1.0",
    "supertest": "^1.2.0"
  }
}
